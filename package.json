{
  "name": "equiteez-home",
  "version": "1.0.0",
  "description": "Marketing site for Equiteez Asset Tokenization",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "main": "build/server/index.js",
  "keywords": [],
  "author": "mavryk",
  "license": "ISC",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "npm run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "sass": "sass --watch app/:app/",
    "graphql-compile": "graphql-codegen --require dotenv/config --config ./codegen.ts dotenv_config_path=./.env; true",
    "graphql-compile-watch": "graphql-codegen --require dotenv/config --config ./codegen.ts dotenv_config_path=./.env",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "npm run build && wrangler pages dev",
    "cf-typegen": "wrangler types"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@dicebear/avatars": "^4.2.5",
    "@dicebear/avatars-bottts-sprites": "^4.2.5",

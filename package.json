    "@remix-run/cloudflare-pages": "^2.8.1",
    "@remix-run/css-bundle": "^2.15.1",
    "@remix-run/node": "^2.9.1",
    "@remix-run/react": "^2.9.1",
    "@remix-run/serve": "^2.9.1",
    "async-retry": "^1.3.3",
    "autoprefixer": "^10.4.19",
    "axios": "^1.7.7",
    "bignumber.js": "^9.1.2",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.0.2",
    "framer-motion": "^11.15.0",
    "fuse.js": "^7.0.0",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "isbot": "^4.1.0",
    "lodash": "^4.17.21",
    "postcss": "^8.4.38",
    "postcss-import": "^16.1.0",
    "react": "^18.2.0",
    "react-css-transition-replace": "^5.0.1",
    "react-dom": "^18.2.0",
    "react-modal": "^3.16.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss": "^3.4.3",
    "tippy.js": "^6.2.6",
    "ts-node": "^10.9.2",
    "use-debounce": "^10.0.3",
    "use-force-update": "^1.0.11",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
